{
  "compileOnSave": false,
  "compilerOptions": {
    "strictPropertyInitialization": false,
    "esModuleInterop": true,
    "baseUrl": "cypress",
    "strict": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "moduleResolution": "nodenext",
    "target": "es6",
    "module": "commonjs",
    "lib": [
      "es6",
      "dom"
    ],
    "types": [
      "node",
      "cypress"
    ],
  },
  "include": [
    "**/*.ts"
  ]
}
